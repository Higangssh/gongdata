{
  "name": "gongdata",
  "version": "0.1.0",
  "description": "TypeScript SDK for Korea Public Data Portal (data.go.kr)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "korea",
    "korean",
    "public-data",
    "data.go.kr",
    "openapi",
    "sdk",
    "typescript",
    "qualification",
    "공공데이터"
  ],
  "author": "SangHee Son",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Higangssh/gongdata.git"
  },
  "bugs": {
    "url": "https://github.com/Higangssh/gongdata/issues"
  },
  "homepage": "https://github.com/Higangssh/gongdata#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.19.0",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.6",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3"
  }
}
